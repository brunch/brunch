{
  "name": "brunch",
  "description": "Fast front-end web app build tool with simple declarative config, seamless incremental compilation for rapid development & opinionated pipeline and workflow",
  "version": "3.0.0-pre",
  "keywords": [
    "assembler",
    "builder",
    "stack",
    "pipeline",
    "build tool",
    "workflow",
    "source map",
    "incremental",
    "config",
    "react",
    "webpack",
    "browserify",
    "grunt",
    "gulp",
    "broccoli",
    "backbone",
    "ember",
    "angular",
    "chaplin",
    "html5"
  ],
  "homepage": "https://brunch.io/",
  "author": "Brunch team (https://brunch.io)",
  "repository": "https://github.com/brunch/brunch.git",
  "bugs": "https://github.com/brunch/brunch/issues",
  "license": "MIT",
  "main": "./lib/index",
  "files": [
    "bin",
    "lib"
  ],
  "bin": {
    "brunch": "bin/brunch"
  },
  "scripts": {
    "lint": "eslint bin/brunch lib test",
    "lint:fix": "eslint --fix bin/brunch lib test",
    "test": "npm run lint && LOGGY_STACKS=1 ava -s --verbose --fail-fast",
    "test:coverage": "nyc ava -s && nyc report --reporter=html"
  },
  "engines": {
    "node": ">= 10.16"
  },
  "dependencies": {
    "anymatch": "^3",
    "anysort": "github:es128/anysort",
    "chokidar": "^3",
    "commander": "~3",
    "commonjs-require-definition": "~0.6.2",
    "debug": "^3.1.0",
    "deppack": "github:brunch/deppack",
    "deps-install": "~0.1",
    "fcache": "~0.3",
    "init-skeleton": "~1.0",
    "loggy": "~1.0.3",
    "micro": "^9.3.4",
    "micro-check-dependencies": "~0.1",
    "mkdirp": "~0.5",
    "serve-brunch": "~0.2",
    "since-app-start": "~0.3",
    "skemata": "~0.1",
    "source-map": "~0.5",
    "universal-path": "^0.1"
  },
  "devDependencies": {},
  "eslintConfig": {
    "extends": "brunch"
  }
}
