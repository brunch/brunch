{
  "name": "brunch",
  "description": "Fast front-end web app build tool with simple declarative config, seamless incremental compilation for rapid development, an opinionated pipeline and workflow, and core support for source maps",
  "version": "2.10.5",
  "keywords": [
    "assembler",
    "builder",
    "stack",
    "pipeline",
    "build tool",
    "workflow",
    "source map",
    "incremental",
    "config",
    "react",
    "webpack",
    "browserify",
    "grunt",
    "gulp",
    "broccoli",
    "backbone",
    "ember",
    "angular",
    "chaplin",
    "html5"
  ],
  "homepage": "http://brunch.io",
  "author": "Brunch team (http://brunch.io)",
  "repository": "https://github.com/brunch/brunch.git",
  "bugs": "https://github.com/brunch/brunch/issues",
  "license": "MIT",
  "main": "lib",
  "bin": {
    "brunch": "bin/brunch"
  },
  "scripts": {
    "lint": "eslint lib",
    "lint:fix": "eslint $npm_package_config_js --fix",
    "test:unit": "mocha test/unit/plugins -r test/unit/__harness",
    "test:e2e": "mocha test/e2e -r test/e2e/_harness",
    "test": "npm run lint && npm run test:unit && npm run test:e2e"
  },
  "config": {
    "js": "bin lib test"
  },
  "dependencies": {
    "anymatch": "~1.3",
    "anysort": "~1",
    "check-dependencies": "~1",
    "chokidar": "^1.7",
    "commander": "~2.11",
    "deps-install": "~0.1",
    "init-skeleton": "^1",
    "joi": "~10.6",
    "loggy": "^1",
    "micro-es7-shim": "paulmillr/micro-es7-shim#new-version",
    "mkdirp": "~0.5",
    "pify": "^3",
    "quickly-copy-file": "^1",
    "serve-brunch": "~0.2",
    "source-map": "~0.5",
    "universal-path": "hellyeahllc/universal-path"
  },
  "devDependencies": {
    "chai": "^4",
    "chai-fs": "^1",
    "eslint": "^4",
    "eslint-config-brunch": "brunch/eslint-config-brunch",
    "mocha": "^3"
  },
  "engines": {
    "node": ">= 6.10"
  },
  "eslintConfig": {
    "extends": "brunch"
  }
}
